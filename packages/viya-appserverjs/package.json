{
  "name": "@sassoftware/viya-appserverjs",
  "version": "1.2.22",
  "description": "Custom app server for Viya apps",
  "author": "Deva Kumaraswamy <deva.kumar@sas.com>",
  "license": "Apache-2.0",
  "main": "./lib/index.js",
  "keywords": [
    "restaf",
    "Viya",
    "SAS",
    "server",
    "RESTAPI"
  ],
  "engines": {
    "node": ">=12.14.0"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/sassoftware/restaf-server#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/sassoftware/restaf-server.git"
  },
  "bin": {
    "@sassoftware/viya-appserverjs": "./lib/cli.js"
  },
  "bugs": {
    "url": "https://github.com/sassoftware/restaf-server/issues"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "start": "cross-env VIYA_SERVER=%vx% node ./lib/cli.js --env=./.env --docker=./Dockerfile",
    "app": "cross-env VIYA_SERVER=%vx% node app.js --env=./.env --docker=./Dockerfile",
    "debug": "cross-env VIYA_SERVER=%vx% node --inspect-brk ./lib/cli.js   --env=./.env --docker=./Dockerfile --appenv=appenv.js",
    "debugapp": "cross-env VIYA_SERVER=%vx% node --inspect-brk app.js  --env=./.env --docker=./Dockerfile --appenv=appenv.js",
    "pub": "npm publish --tag test --access public",
    "lint": "npx eslint --fix src/*.js"
  },
  "dependencies": {
    "@hapi/bell": "^12.1.1",
    "@hapi/boom": "^9.1.1",
    "@hapi/cookie": "^11.0.2",
    "@hapi/h2o2": "^9.0.2",
    "@hapi/hapi": "^20.0.3",
    "@hapi/inert": "^6.0.3",
    "@hapi/joi": "^17.1.1",
    "@hapi/vision": "^6.0.1",
    "@sassoftware/viya-server-core": "1.2.22",
    "axios": "^0.21.1",
    "core-js": "^3.8.1",
    "debug": "^4.3.1",
    "docker-file-parser": "^1.0.5",
    "eslint-config-prettier": "^7.1.0",
    "handlebars": "^4.7.6",
    "hapi-pino": "^8.3.0",
    "hapi-require-https": "^5.0.0",
    "hapi-swagger": "^14.0.0",
    "is-docker": "^2.1.1",
    "jwt-decode": "^3.1.2",
    "minimist": "^1.2.5",
    "node-cache-promise": "^1.0.2",
    "only": "0.0.2",
    "qs": "^6.9.4",
    "regenerator-runtime": "^0.13.7",
    "request-debug": "^0.2.0",
    "selfsigned": "^1.10.8",
    "should-send-same-site-none": "^2.0.5",
    "tcp-ping": "^0.1.1",
    "uuid": "^8.3.2",
    "yargs": "^16.2.0"
  }
}
