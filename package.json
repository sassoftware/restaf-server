{
  "name": "@sassoftware/restaf-server",
  "version": "1.2.54",
  "description": "app and api server for SAS Viya user applications",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "core:build": "cd packages/viya-server-core && yarn build",
    "appjs:build": "cd packages/viya-appserverjs && yarn build",
    "appjs:start": "cd packages/apptest && yarn start",
    "appjs:istart": "cd packages/apptest && yarn istart",
    "appjs:app": "cd packages/viya-appserverjs && yarn app",
    "appjs:appenv": "cd packages/viya-appserverjs && yarn appenv",
    "appjs:debug": "cd packages/apptest && yarn debug",
    "apijs:debug": "cd packages/apitest && yarn debug",
    "apijs:build": "cd packages/viya-apiserverjs && yarn build",
    "apijs:start": "cd packages/apitest && yarn start",
    "build": "yarn core:build && yarn appjs:build && yarn apijs:build",
    "core:pub": "cd packages/viya-server-core  &&  npm publish  --tag test",
    "appjs:pub": "cd packages/viya-appserverjs && npm publish --tag test",
    "apijs:pub": "cd packages/viya-apiserverjs && npm publish --tag test",
    "create:pub": "cd packages/create-viya-api && npm publish --tag test",
    "base:pub": "cd packages/viya-api-base     && npm  publish --tag test",
    "pub": "npm run core:pub && npm run appjs:pub && npm run apijs:pub && npm run create:pub && npm run base:pub",
    "format": "prettier-eslint --eslint-config-path ./.eslintrc.json --write '**/*.js'",
    "lint": "eslint . --ext .js --fix"
  },
  "repository": "git+https://github.com/sassoftware/restaf-server.git",
  "keywords": [
    "REST",
    "API",
    "Viya",
    "restaf"
  ],
  "author": "Deva Kumar",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sassoftware/restaf-server/issues"
  },
  "homepage": "https://github.com/sassoftware/restaf-server#readme",
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-proposal-do-expressions": "^7.14.5",
    "@babel/plugin-proposal-export-default-from": "^7.14.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.14.5",
    "@babel/plugin-proposal-function-bind": "^7.14.5",
    "@babel/plugin-proposal-function-sent": "^7.14.5",
    "@babel/plugin-proposal-json-strings": "^7.14.5",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.14.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-numeric-separator": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-proposal-pipeline-operator": "^7.14.5",
    "@babel/plugin-proposal-throw-expressions": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-regenerator": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/plugin-transform-spread": "^7.14.6",
    "@babel/plugin-transform-strict-mode": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.28.0",
    "prettier": "^2.3.1",
    "prettier-eslint": "^12.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "rimraf": "^3.0.2",
    "set-versions": "^1.0.3",
    "shx": "^0.3.3"
  },
  "workspaces": {
    "packages": [
      "packages/viya-apiserverjs",
      "packages/viya-appserverjs",
      "packages/viya-server-core",
      "packages/create-viya-api",
      "packages/viya-api-base",
      "packages/apptest",
      "packages/apitest"
    ]
  },
  "dependencies": {
    "@sassoftware/restaf": "test",
    "@sassoftware/restaflib": "test",
    "cross-spawn": "^7.0.3",
    "hapi-swagger": "^14.2.0",
    "user": "*",
    "uuid": "^8.3.2"
  },
  "resolutions": {
    "yargs-parser": ">=18.1.2",
    "underscore": "1.12.1",
    "trim-newlines": "3.0.1",
    "glob-parent": ">=5.1.2"
  }
}
