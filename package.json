{
  "name": "@sassoftware/restaf-server",
  "version": "7.4.1",
  "description": "Custom app server for Viya apps",
  "author": "Deva Kumaraswamy <deva.kumar@sas.com>",
  "license": "Apache-2.0",
  "main": "./lib/index.js",
  "keywords": [
    "restaf",
    "Viya",
    "SAS",
    "server",
    "RESTAPI"
  ],
  "engines": {
    "node": ">=8.9.0"
  },
  "bin": {
    "@sassoftware/restaf-server": "cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sassoftware/restaf-server.git"
  },
  "bugs": {
    "url": "https://github.com/sassoftware/restaf-server/issues"
  },
  "scripts": {
    "static": "node cli --env=./env/static/overrides.env --docker=./Dockerfile",
    "custom": "node cli --env=./env/implicitCustom/overrides.env --docker=./Dockerfile",
    "implicit": "node cli --env=./env/implicit/overrides.env --docker=./Dockerfile",
    "code": "node cli --env=./env/code/overrides.env --docker=./Dockerfile",
    "debugimplicit": "node --inspect-brk cli --env=./env/implicit/overrides.env --docker=./Dockerfile --appenv=appenv.js",
    "debugcode": "node --inspect-brk cli --env=./env/code/overrides.env --docker=./Dockerfile --appenv=appenv.js",
    "login": "node cli --env=./env/login/overrides.env --docker=./Dockerfile --appenv=appenv.js",
    "build": "rimraf lib && babel src --out-dir lib",
    "indocker": "node cli",
    "dkrbuild": "docker build -f Dockerfile -t viyaapp .",
    "dkrapp": "docker run -p 8080:8080 --env-file /env/code/overrides.env  -t viyaapp",
    "dkrappi": "docker run -p 8080:8080 --env-file overridesimplicit.env  -t viyaapp",
    "lint": "npx eslint --fix src/*.js"
  },
  "dependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@hapi/bell": "^11.1.0",
    "@hapi/boom": "^8.0.1",
    "@hapi/cookie": "^10.1.2",
    "@hapi/hapi": "^18.4.1",
    "@hapi/inert": "^5.2.2",
    "@hapi/joi": "^16.1.8",
    "@hapi/vision": "^5.5.4",
    "core-js": "^3.6.1",
    "debug": "^4.1.1",
    "docker-file-parser": "^1.0.4",
    "eslint-config-prettier": "^6.8.0",
    "hapi-swagger": "^11.1.0",
    "is-docker": "^2.0.0",
    "jwt-decode": "^2.2.0",
    "minimist": "^1.2.0",
    "only": "0.0.2",
    "qs": "^6.9.1",
    "regenerator-runtime": "^0.13.3",
    "request-debug": "^0.2.0",
    "tcp-ping": "^0.1.1",
    "uuid": "^3.3.3",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-do-expressions": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-proposal-function-bind": "^7.8.3",
    "@babel/plugin-proposal-function-sent": "^7.8.3",
    "@babel/plugin-proposal-json-strings": "^7.8.3",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-proposal-pipeline-operator": "^7.8.3",
    "@babel/plugin-proposal-throw-expressions": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.8.3",
    "@babel/plugin-transform-regenerator": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/plugin-transform-spread": "^7.8.3",
    "@babel/plugin-transform-strict-mode": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "core-js": "^3.6.4",
    "eslint": "^6.8.0",
    "rimraf": "^3.0.0"
  }
}
