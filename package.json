{
  "name": "@sassoftware/restaf-server",
  "version": "1.3.0",
  "description": "app and api server for SAS Viya user applications",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "core:build": "cd packages/viya-server-core && yarn build",
    "appjs:build": "cd packages/viya-appserverjs && yarn build",
    "appjs:start": "cd packages/apptest && yarn start",
    "appjs:istart": "cd packages/apptest && yarn istart",
    "appjs:app": "cd packages/viya-appserverjs && yarn app",
    "appjs:appenv": "cd packages/viya-appserverjs && yarn appenv",
    "appjs:debug": "cd packages/apptest && yarn debug",
    "apijs:debug": "cd packages/apitest && yarn debug",
    "apijs:build": "cd packages/viya-apiserverjs && yarn build",
    "apijs:start": "cd packages/apitest && yarn start",
    "build": "yarn core:build && yarn appjs:build && yarn apijs:build",
    "core:pub": "cd packages/viya-server-core  &&  npm publish  --tag test public --access public",
    "appjs:pub": "cd packages/viya-appserverjs && npm publish --tag test --access public",
    "apijs:pub": "cd packages/viya-apiserverjs && npm publish --tag test --access public",
    "create:pub": "cd packages/create-viya-api && npm publish --tag test --access public",
    "base:pub": "cd packages/viya-api-base     && npm  publish --tag test --access public",
    "pub": "npm run core:pub && npm run appjs:pub && npm run apijs:pub && npm run create:pub && npm run base:pub",
    "format": "prettier-eslint --eslint-config-path ./.eslintrc.json --write '**/*.js'",
    "lint": "eslint . --ext .js --fix"
  },
  "repository": "git+https://github.com/sassoftware/restaf-server.git",
  "keywords": [
    "REST",
    "API",
    "Viya",
    "restaf"
  ],
  "author": "Deva Kumar",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sassoftware/restaf-server/issues"
  },
  "homepage": "https://github.com/sassoftware/restaf-server#readme",
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.0",
    "@babel/plugin-proposal-do-expressions": "^7.16.7",
    "@babel/plugin-proposal-export-default-from": "^7.16.7",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.7",
    "@babel/plugin-proposal-function-bind": "^7.16.7",
    "@babel/plugin-proposal-function-sent": "^7.16.7",
    "@babel/plugin-proposal-json-strings": "^7.16.7",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.16.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
    "@babel/plugin-proposal-numeric-separator": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-proposal-pipeline-operator": "^7.16.7",
    "@babel/plugin-proposal-throw-expressions": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-regenerator": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/plugin-transform-spread": "^7.16.7",
    "@babel/plugin-transform-strict-mode": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/eslint-parser": "7.17.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.8.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "shx": "^0.3.4"
  },
  "private": true,
  "workspaces": {
    "packages": [
      "packages/viya-apiserverjs",
      "packages/viya-appserverjs",
      "packages/viya-server-core",
      "packages/create-viya-api",
      "packages/viya-api-base",
      "packages/apptest",
      "packages/apitest"
    ]
  },
  "dependencies": {
    "@hapi/bell": "^12.3.0",
    "@hapi/boom": "^9.1.4",
    "@hapi/cookie": "^11.0.2",
    "@hapi/h2o2": "^9.1.0",
    "@hapi/hapi": "^20.2.1",
    "@hapi/inert": "^6.0.5",
    "@hapi/joi": "^17.1.1",
    "@hapi/vision": "^6.1.0",
    "axios": "^0.25.0",
    "core-js": "^3.21.0",
    "cross-spawn": "^7.0.3",
    "debug": "^4.3.3",
    "docker-file-parser": "^1.0.7",
    "eslint-config-prettier": "^8.3.0",
    "handlebars": "^4.7.7",
    "hapi-pino": "^9.1.2",
    "hapi-require-https": "^5.0.0",
    "hapi-swagger": "^14.2.4",
    "is-docker": "^3.0.0",
    "jwt-decode": "^3.1.2",
    "minimist": "^1.2.5",
    "node-cache-promise": "^1.0.2",
    "only": "0.0.2",
    "pino-pretty": "^7.5.1",
    "qs": "^6.10.3",
    "regenerator-runtime": "^0.13.9",
    "request-debug": "^0.2.0",
    "selfsigned": "^2.0.0",
    "should-send-same-site-none": "^2.0.5",
    "tcp-ping": "^0.1.1",
    "uuid": "^8.3.2",
    "yargs": "17.3.1"
  },
  "resolutions": {
    "node-forge": ">=1.0.0",
    "underscore": "1.13.1",
    "trim-newlines": "3.0.1",
    "glob-parent": ">=5.1.2",
    "swagger-ui": "4.5.0",
    "swagger-ui-dist": "^4.5.0",
    "validator": ">=13.7.0"
  }
}
