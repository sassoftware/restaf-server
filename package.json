{
  "name": "restaf-server",
  "version": "7.0.0",
  "description": "Custom app server for Viya apps",
  "author": "Deva Kumaraswamy <deva.kumar@sas.com>",
  "license": "Apache-2.0",
  "main": "./lib/index.js",
  "keywords": [
    "restaf",
    "Viya",
    "SAS",
    "server",
    "RESTAPI"
  ],
  "engines": {
    "node": ">=8.9.0"
  },
  "bin": {
    "restaf-server": "cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sassoftware/restaf-server.git"
  },
  "bugs": {
    "url": "https://github.com/sassoftware/restaf-server/issues"
  },
  "scripts": {
    "start": "node cli --env=./env/myapp.env --appenv=appenv.js",
    "app": "node app --env=./env/myapp.env --appenv=appenv.js",
    "implicit": "node cli --env=./implicit/app.env --docker=./implicit/Dockerfile",
    "proxy": "node cli --env=./proxy/app.env --docker=./proxy/Dockerfile --appenv=appenv.js",
    "code": "node cli --env=./code/app.env --docker=./code/Dockerfile --appenv=appenv.js",
    "build": "rimraf lib && babel src --out-dir lib",
    "indocker": "node cli",
    "dkrbuild": "docker build -f Dockerfile -t cms .",
    "dkrapp": "docker run -p 8080:8080 --env-file app.env  -t cms",
    "lint": "./node_modules/.bin/eslint --fix src/*.js"
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@hapi/bell": "^10.1.1",
    "@hapi/boom": "^7.4.3",
    "@hapi/cookie": "^10.1.1",
    "@hapi/hapi": "^18.3.2",
    "@hapi/inert": "^5.2.1",
    "babel-polyfill": "^6.26.0",
    "debug": "^4.1.1",
    "docker-file-parser": "^1.0.4",
    "eslint-config-prettier": "^6.1.0",
    "is-docker": "^2.0.0",
    "minimist": "^1.2.0",
    "only": "0.0.2",
    "qs": "^6.8.0",
    "request": "^2.88.0",
    "request-debug": "^0.2.0",
    "tcp-ping": "^0.1.1",
    "uuid": "^3.3.3",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-do-expressions": "^7.5.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.5.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.5.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "eslint": "^6.2.2",
    "prettyjson": "^1.2.1",
    "rimraf": "^3.0.0",
    "webpack-cli": "^3.3.7"
  }
}
